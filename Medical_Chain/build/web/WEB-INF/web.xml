<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>NewFTMI</servlet-name>
        <servlet-class>NewFTMI</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Scan_QR</servlet-name>
        <servlet-class>Scan_QR</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Upload_Report</servlet-name>
        <servlet-class>Upload_Report</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Try</servlet-name>
        <servlet-class>Try</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin_registration</servlet-name>
        <servlet-class>admin_registration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Admin_Registration</servlet-name>
        <servlet-class>Admin_Registration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Scan_QR_User</servlet-name>
        <servlet-class>Scan_QR_User</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewFTMI</servlet-name>
        <url-pattern>/NewFTMI</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Scan_QR</servlet-name>
        <url-pattern>/Scan_QR</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Upload_Report</servlet-name>
        <url-pattern>/Upload_Report</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Try</servlet-name>
        <url-pattern>/Try</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admin_registration</servlet-name>
        <url-pattern>/admin_registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Admin_Registration</servlet-name>
        <url-pattern>/Admin_Registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Scan_QR_User</servlet-name>
        <url-pattern>/Scan_QR_User</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            90
        </session-timeout>
    </session-config>
</web-app>
